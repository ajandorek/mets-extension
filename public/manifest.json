{
  "manifest_version": 2,

  "name": "Do the Mets play today?",
  "description": "This extension lets a user know whehter or not the Mets are playing today.",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "icons/favicon-16x16.png",
    "48": "icons/favicon-32x32.png",
    "128": "icons/favicon.ico"
  },
  "permissions": []
}
